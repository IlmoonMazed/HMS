\n    const { data, error } = await supabase\r\n      .from(\"User\")\r\n      .insert([\r\n        {\r\n          Name: name,\r\n          Email: email,\r\n          Password: password, // ⚠️ You should hash this in a real app\r\n          Role_ID: roleId,\r\n          Registration_Date: new Date().toISOString(),\r\n          Gender: gender,\r\n          Date_of_Birth: dob,\r\n          Phone: phone,\r\n          Address: address,\r\n        }\r\n      ])\r\n      .select()\r\n      .single();\r\n\r\n    if (error) {\r\n      console.log(error);\r\n      setFormError(\"Could not complete sign up.\");\r\n    } else {\r\n      setFormError(null);\r\n      navigate(\"/SignIn\");\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"signUp\">\r\n      <form onSubmit={handleSubmit}>\r\n        <label>Name:</label>\r\n        <input type=\"text\" value={name} onChange={(e) => setName(e.target.value)} />\r\n\r\n        <label>Email:</label>\r\n        <input type=\"email\" value={email} onChange={(e) => setEmail(e.target.value)} />\r\n\r\n        <label>Password:</label>\r\n        <input type=\"password\" value={password} onChange={(e) => setPassword(e.target.value)} />\r\n\r\n        <label>Confirm Password:</label>\r\n        <input type=\"password\" value={confirmPassword} onChange={(e) => setConfirmPassword(e.target.value)} />\r\n\r\n        <label>Date of Birth:</label>\r\n        <input type=\"date\" value={dob} onChange={(e) => setDob(e.target.value)} />\r\n\r\n        <label>Phone:</label>\r\n        <input type=\"text\" value={phone} onChange={(e) => setPhone(e.target.value)} />\r\n\r\n        <label>Address:</label>\r\n        <input type=\"text\" value={address} onChange={(e) => setAddress(e.target.value)} />\r\n\r\n        <label>Gender:</label>\r\n        <select value={gender} onChange={(e) => setGender(e.target.value)}>\r\n          <option value=\"M\">Male</option>\r\n          <option value=\"F\">Female</option>\r\n        </select>\r\n\r\n        <label>Role:</label>\r\n        <select value={roleId} onChange={(e) => setRoleId(e.target.value)}>\r\n          <option value=\"1\">Doctor</option>\r\n          <option value=\"2\">Patient</option>\r\n          <option value=\"3\">Employee</option>\r\n        </select>\r\n\r\n        <button type=\"submit\">Sign Up</button>\r\n\r\n        {formError && <p className=\"error\">{formError}</p>}\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default SignUp;\r\n","C:\\Users\\ilmoo\\OneDrive\\Desktop\\file\\HMS\\HMS\\src\\pages\\UpdateProfile2.js",[],[],{"ruleId":"64","severity":1,"message":"65","line":1,"column":10,"nodeType":"66","messageId":"67","endLine":1,"endColumn":19},{"ruleId":"64","severity":1,"message":"65","line":1,"column":10,"nodeType":"66","messageId":"67","endLine":1,"endColumn":19},{"ruleId":"64","severity":1,"message":"68","line":7,"column":9,"nodeType":"66","messageId":"67","endLine":7,"endColumn":17},{"ruleId":"64","severity":1,"message":"65","line":1,"column":10,"nodeType":"66","messageId":"67","endLine":1,"endColumn":19},{"ruleId":"64","severity":1,"message":"68","line":7,"column":9,"nodeType":"66","messageId":"67","endLine":7,"endColumn":17},{"ruleId":"64","severity":1,"message":"69","line":52,"column":13,"nodeType":"66","messageId":"67","endLine":52,"endColumn":17},"no-unused-vars","'useParams' is defined but never used.","Identifier","unusedVar","'navigate' is assigned a value but never used.","'data' is assigned a value but never used."]